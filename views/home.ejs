<%- include("partials/header")  %> 

<h1>Home</h1>
<p><%= startingContent %></p>
<div class="posts">
    <% newPosts.forEach(function(post) { %>
        <h1><%= post.title %> </h1>
        <p><%= post.body.length>100 ? post.body.substring(0, 100) + "..." : post.body %> 
            <a href="/posts/<%= post.title %> ">Read More</a>
        </p>
        <% console.log(post.title) %> 
    <% }) %>
</div>

<%- include("partials/footer") %> 